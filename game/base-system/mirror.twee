<<widget "mirrorMakeupPart">>
<<set _itemType = $args[0]>>
<<set _singular = $args[1]>>
<<set _plural = $args[2]>>
<<set _cssClass = $args[3]>>

<<set _items = $makeup.owned[_itemType].filter(x => x.count > 0)>>
<<if $makeup.owned[_itemType].length == 0>>
